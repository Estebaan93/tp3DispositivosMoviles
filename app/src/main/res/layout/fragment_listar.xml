<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.producto.ListarFragment">


  <androidx.recyclerview.widget.RecyclerView
      android:id="@+id/idLista"
      android:layout_width="409dp"
      android:layout_height="575dp"
      tools:ignore="MissingConstraints"
      tools:layout_editor_absoluteX="1dp"
      tools:layout_editor_absoluteY="36dp" />

  <TextView
      android:id="@+id/TvVacia"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:text="TextView"
      app:layout_constraintBottom_toBottomOf="@+id/idLista"
      app:layout_constraintEnd_toEndOf="@+id/idLista"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="@+id/idLista"
      app:layout_constraintTop_toTopOf="@+id/idLista"
      app:layout_constraintVertical_bias="0.197" />
</androidx.constraintlayout.widget.ConstraintLayout>